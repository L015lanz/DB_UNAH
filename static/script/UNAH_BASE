--------------------------------------------------------
--  DDL for Procedure SP_SIGUIENTE_PERIODO
--------------------------------------------------------
set define off;

  CREATE OR REPLACE EDITIONABLE PROCEDURE "C##DBAUNAH"."SP_SIGUIENTE_PERIODO" (
  p_CURSOR OUT SYS_REFCURSOR,
    p_DURACION_ID IN varchar2
  
) AS
    p_PERIODO NUMBER;
    p_A헲  VARCHAR2(4);
    v_next_period NUMBER;
    v_next_year VARCHAR2(4);

    CURSOR PERIODO_ACTUAL IS SELECT MAX(periodo) AS max_periodo, a絪
    FROM periodo
    WHERE duracion_id = p_DURACION_ID
    GROUP BY a絪
    ORDER BY a絪 desc;
BEGIN
    -- Calcular el siguiente per癌do

    OPEN PERIODO_ACTUAL;

    FETCH PERIODO_ACTUAL INTO p_PERIODO,p_A헲;

    IF p_DURACION_ID = 1 THEN
        IF p_PERIODO = 3 THEN
            v_next_period := 1;
            v_next_year := TO_CHAR(TO_NUMBER(p_A헲) + 1);
        ELSE
            v_next_period := p_PERIODO + 1;
            v_next_year := p_A헲;
        END IF;
    ELSIF p_DURACION_ID = 2 THEN
        IF p_PERIODO = 2 THEN
            v_next_period := 1;
            v_next_year := TO_CHAR(TO_NUMBER(p_A헲) + 1);
        ELSE
            v_next_period := p_PERIODO + 1;
            v_next_year := p_A헲;
        END IF;
    ELSE
        v_next_period := p_PERIODO + 1;
        v_next_year := p_A헲;
    END IF;
    CLOSE PERIODO_ACTUAL;
    -- Abrir un cursor para el siguiente per癌do
    OPEN p_CURSOR FOR
    SELECT v_next_period AS PERIODO, v_next_year AS ANIO FROM DUAL;
END;

